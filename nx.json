{
  "affected": {
    "defaultBase": "master"
  },
  "extends": "nx/presets/npm.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "prepare": {
      "dependsOn": ["^prepare"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "cacheableOperations": ["build", "lint", "prebuild", "prepare", "preversion", "test", "storybook"]
      },
      "runner": "nx/tasks-runners/default"
    }
  }
}
