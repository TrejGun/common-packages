    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-standard": "16.0.2",
    "eslint-import-resolver-ts": "0.4.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-standard": "4.1.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "UNLICENSED",
  "name": "@trejgun/common-packages",
  "repository": {
    "type": "git",
    "url": "https://github.com/trejgun/common-packages"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build --stream",
    "clean": "sh scripts/clean.sh",
    "kill": "sudo pkill -9 node",
    "lint": "lerna run lint --stream --parallel",
    "precommit": "lint-staged",
    "prettier": "prettier \"packages/**/*.{ts,tsx}\" --write",
    "registry": "lerna publish",
    "release": "npm run release:version && npm run release:publish",
    "release:publish": "lerna publish from-package --yes --exact --ignore-scripts --registry=https://npm.pkg.github.com/",
    "release:version": "lerna version --yes --exact --conventional-commits",
    "test": "lerna run test --stream"
  },
  "version": "0.0.1"
}
